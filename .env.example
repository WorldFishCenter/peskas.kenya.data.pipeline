# Peskas Kenya Data Pipeline Environment Variables
# Copy this file to .env and fill in your actual values

# WCS Kobo Credentials
KOBO_ASSET_ID=your_kobo_asset_id
KOBO_ASSET_ID_PRICE=your_kobo_asset_id_price
KOBO_USERNAME=your_kobo_username
KOBO_PASSWORD=your_kobo_password

# WCS Kobo KF Credentials
KOBO_ASSET_ID_KF=your_kobo_asset_id_kf
KOBO_ASSET_ID_PRICE_KF=your_kobo_asset_id_price_kf
KOBO_USERNAME_KF=your_kobo_username_kf
KOBO_PASSWORD_KF=your_kobo_password_kf

# KEFS Kobo Credentials
KEFS_KOBO_ASSET_ID=your_kefs_kobo_asset_id
KEFS_KOBO_USERNAME=your_kefs_kobo_username
KEFS_KOBO_PASSWORD=your_kefs_kobo_password

# PDS (GPS) Credentials
PDS_TOKEN=your_pds_token
PDS_SECRET=your_pds_secret

# Google Cloud Storage
# For JSON content, paste the entire service account key JSON as a single line
GCP_SA_KEY={"type":"service_account","project_id":"..."}

# MongoDB
MONGODB_CONNECTION_STRING=mongodb://username:password@host:port/database

# Google Sheets
GOOGLE_SHEET_ID=your_google_sheet_id

# Airtable
AIRTABLE_BASE_ID=your_airtable_base_id
AIRTABLE_TOKEN=your_airtable_token

# R Configuration Environment
# Options: local, default, production
R_CONFIG_ACTIVE=local
